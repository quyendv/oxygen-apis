{
  "name": "oxygen-apis",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node --exec babel-node server.js",
    "start:dev": "nodemon --exec babel-node server.js",
    "lint": "eslint \"{src,test}/**/*.js\"",
    "lint:fix": "eslint \"{src,test}/**/*.js\" --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "prepare": "husky install",
    "migration:run": "npx sequelize-cli db:migrate",
    "migration:revert": "npx sequelize-cli db:migrate:undo",
    "test": "mocha"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "npm run format",
      "git add ."
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chai": "^4.3.10",
    "chai-http": "^4.4.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^11.11.0",
    "http-errors": "^2.0.0",
    "joi": "^17.11.0",
    "mocha": "^10.2.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "2.6.6",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "save-dev": "^0.0.1-security",
    "sequelize": "^6.34.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.2",
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "chai-things": "^0.2.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "mochawesome": "^7.1.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "sequelize-cli": "^6.6.2"
  }
}
